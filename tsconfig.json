{
  "compilerOptions": {
    "jsx": "react-jsx",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "baseUrl": "./src",
    "paths": {
      "@api/*": ["share/api/*"],
      "@env": ["share/env"],
      "@component/*": ["share/component/*"],
      "@hook/*": ["share/hook/*"],
      "@store/*": ["share/store/*"],
      "@url": ["share/url"]
    }
  }
}